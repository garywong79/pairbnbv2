#  
# ---------------- Glimpse Command Switches -------------------
# '-i' = Case Insentive  '-w' = Word Matches only  -l Filename Matches Only
# '-X' = Print Filename when match is in index but not on disk!
#
alias gi='glimpse -i '         # Find Text  (Case Insenstive)
#
alias gil='glimpse -i -l '     # Find Text  (Case Insenstive) (Just Matching Filenames)
alias giwl='glimpse -i -w -l ' # Find Words (Case Insenstive) (Just Matching Filenames)
alias gwl='glimpse -w -l'      # Find Words (Case Counts)     (Just Matching Filenames) 
#
alias giw='glimpse -i -w '     # Find Words (Case Insenstive)
alias gw='glimpse -w '         # Find Words (Case Counts)
#
alias glw='glimpse -l -w '     # Find Full Filenames  (Case Counts)
alias gl='glimpse -l '         # Find Filenames       (Case Counts)
#
alias gilw='glimpse -l -i -w ' # Find Full Filenames  (Case Insensitive)
alias gil='glimpse -l -i '     # Find Filenames       (Case Insensitive)
